@using TravelTripProject.Models.Classes
@model List<Blog>

<div class="padding">
    <h4>En Son Gezdiklerim</h4>
    <ul class="list list-pad">
        @{int i=0;} 
        @foreach (var item in Model)
        {

            <li class="list-bg"><span>@{i++;}@i.</span><a href="#">@item.Baslik</a></li>

        }

    </ul>
    <a href="/Blog/Index" class="link1">Diğer Yazılara Gözat</a>
</div>
